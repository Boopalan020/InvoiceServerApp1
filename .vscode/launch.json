{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "cds watch --profile hybrid",
      "request": "launch",
      "type": "node",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "cds",
      "args": [
        "watch",
        "--profile",
        "hybrid"
      ],
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
    {
      "name": "cds watch",
      "request": "launch",
      "type": "node",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "cds",
      "args": [
        "watch"
      ],
      "skipFiles": [
        "<node_internals>/**"
      ]
    }
  ]
}